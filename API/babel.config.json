{
    "presets": [
        "@babel/preset-typescript",
        [
            "@babel/preset-env", {
            "targets": {
                "node": 16
                }
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver", {
                "root": ["./src"],
                "alias": {
                    "*": "./*"
                },
                "extensions": [".ts"]
            }
        ]
    ]
}